{
  "name": "intervuai",
  "version": "0.1.0",
  "description": "Autonomous AI conversational agent for technical interviews",
  "private": true,
  "workspaces": [
    "backend",
    "frontend"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev --workspace=backend\" \"npm run dev --workspace=frontend\"",
    "dev:backend": "npm run dev --workspace=backend",
    "dev:frontend": "npm run dev --workspace=frontend",
    "build": "npm run build --workspace=backend && npm run build --workspace=frontend",
    "build:backend": "npm run build --workspace=backend",
    "build:frontend": "npm run build --workspace=frontend",
    "start": "npm start --workspace=backend",
    "lint": "npm run lint --workspace=backend && npm run lint --workspace=frontend",
    "test": "npm test --workspace=backend && npm test --workspace=frontend"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "ai",
    "interviews",
    "technical",
    "conversational",
    "gpt4",
    "deepgram",
    "elevenlabs"
  ],
  "author": "IntervuAI Team",
  "license": "MIT"
}
